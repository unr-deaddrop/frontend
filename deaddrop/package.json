{
	"name": "deaddrop",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite --host --port 5173",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"prepare": "npm run create-smui-theme && npm run smui-theme-light && npm run smui-theme-dark",
		"create-smui-theme": "npx smui-theme template src/theme",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@mdi/js": "^7.4.47",
		"@playwright/test": "^1.28.1",
		"@smui/common": "^7.0.0-beta.18",
		"@smui/floating-label": "^7.0.0-beta.18",
		"@smui/line-ripple": "^7.0.0-beta.18",
		"@smui/list": "^7.0.0-beta.18",
		"@smui/menu": "^7.0.0-beta.18",
		"@smui/menu-surface": "^7.0.0-beta.18",
		"@smui/notched-outline": "^7.0.0-beta.18",
		"@smui/ripple": "^7.0.0-beta.18",
		"@smui/select": "^7.0.0-beta.18",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.69.5",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"smui-theme": "^7.0.0-beta.18",
		"vitest": "^0.32.2"
	},
	"type": "module",
	"dependencies": {
		"chart.js": "^4.4.1",
		"chartjs": "^0.3.24",
		"dotenv": "^16.3.1",
		"svelte-chartjs": "^3.1.5",
		"svelte-jsonschema-form": "github:webgme/svelte-jsonschema-form"
	}
}
