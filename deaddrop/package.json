		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.69.5",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"sveltekit-superforms": "^2.6.2",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
